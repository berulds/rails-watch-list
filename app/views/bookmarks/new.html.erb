<h1>Create a new Bookmark</h1>

<%= simple_form_for [@list, @bookmark] do |f| %>
  <%= f.association :movie, collection: Movie.all, input_html: { multiple: true, class: "multiple-select" } %>
  <%= f.input :comment %>
  <%= f.submit class: "btn btn-primary" %>
<% end %>




